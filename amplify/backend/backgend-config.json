{
    "storage": {
      "blogWebsiteTable": {
        "service": "DynamoDB",
        "providerPlugin": "awscloudformation"
      }
    },
    "function": {
      "blogWebsiteLambda": {
        "build": true,
        "providerPlugin": "awscloudformation",
        "service": "Lambda",
        "dependsOn": [
          {
            "category": "storage",
            "resourceName": "blogWebsiteTable",
            "attributes": [
              "Name",
              "Arn"
            ],
            "attributeEnvMap": {
              "Name": "TABLE_NAME",
              "Arn": "TABLE_ARN"
            }
          }
        ]
      }
    },
    "auth": {
      "blogwebsite": {
        "service": "Cognito",
        "serviceType": "managed",
        "providerPlugin": "awscloudformation",
        "dependsOn": [],
        "frontendAuthConfig": {
          "socialProviders": [],
          "usernameAttributes": [],
          "signupAttributes": [
            "EMAIL"
          ],
          "passwordProtectionSettings": {
            "passwordPolicyMinLength": 8,
            "passwordPolicyCharacters": []
          },
          "mfaConfiguration": "OFF",
          "mfaTypes": [
            "SMS"
          ],
          "verificationMechanisms": [
            "EMAIL"
          ]
        }
      }
    },
    "api": {
      "blogApi": {
        "service": "API Gateway",
        "providerPlugin": "awscloudformation",
        "dependsOn": [
          {
            "category": "function",
            "resourceName": "blogWebsiteLambda",
            "attributes": [
              "Name",
              "Arn"
            ]
          }
        ]
      }
    }
  }